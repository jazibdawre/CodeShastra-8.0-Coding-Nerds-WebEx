{
  "manifest_version": 2,

  "name": "Zepp",
  "description": "Web Helper for Zepp",
  "version": "1.0",

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "options_page": "options.html",

  "permissions": ["storage", "tabs", "notifications"],

  "browser_action": {
    "default_icon": "img/Zepp-logos_white-ico.png",
    "default_popup": "index.html"
  }
}
